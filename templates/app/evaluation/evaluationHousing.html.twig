{% extends "model/collapseCard.html.twig" %}

{% set id = "eval_housing" %}

{% block card_title %}Logement - Hébergement{% endblock %}

{% block card_body %}

<div class="js-evalGroup" data-supportId="g-{{form.vars.value.supportGroup.id}}">
    <div class="form-group row">
        <div class="col-md-6">{{ form_row(form.evalHousingGroup.housingStatus) }}</div>
        {# <div class="col-md-6 js-housingAccessType d-block">{{ form_row(form.evalHousingGroup.housingAccessType) }}</div> #}
        <div class="col-md-6 js-housingStatus d-block">{{ form_row(form.evalHousingGroup.housingArrivalDate) }}</div>
    </div>
    <div class="js-housingStatus d-block">
        <div class="form-group row">
            <div class="col-md-6">{{ form_row(form.evalHousingGroup.expulsionInProgress) }}</div>
            <div class="col-md-6 js-expulsionInProgress d-block">{{ form_row(form.evalHousingGroup.publicForce) }}</div>
            <div class="col-md-6 js-expulsionInProgress d-block">{{ form_row(form.evalHousingGroup.publicForceDate) }}</div>
            <div class="col-md-6 js-expulsionInProgress d-block">{{ form_row(form.evalHousingGroup.expulsionComment) }}</div>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">{{ form_row(form.evalHousingGroup.siaoRequest) }}</div>
        <div class="col-md-6 js-siaoRequest d-block">{{ form_row(form.evalHousingGroup.siaoRequestDate) }}</div>
        <div class="col-md-6 js-siaoRequest d-block">{{ form_row(form.evalHousingGroup.siaoUpdatedRequestDate) }}</div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">{{ form_row(form.evalHousingGroup.socialHousingRequest) }}</div>
        <div class="col-md-6 js-socialHousingRequest d-block">{{ form_row(form.evalHousingGroup.socialHousingRequestId) }}</div>
        <div class="col-md-6 js-socialHousingRequest d-block">{{ form_row(form.evalHousingGroup.socialHousingRequestDate) }}</div>
        <div class="col-md-6 js-socialHousingRequest d-block">{{ form_row(form.evalHousingGroup.socialHousingUpdatedRequestDate) }}</div>
        <div class="col-md-6 js-socialHousingRequest d-block">{{ form_row(form.evalHousingGroup.housingWishes) }}</div>
        <div class="col-md-6 js-socialHousingRequest d-block">{{ form_row(form.evalHousingGroup.citiesWishes) }}</div>
        <div class="col-md-6 js-socialHousingRequest d-block">{{ form_row(form.evalHousingGroup.specificities) }}</div>
        <div class="col-md-6 js-socialHousingRequest d-block">{{ form_row(form.evalHousingGroup.syplo) }}</div>
        <div class="col-md-6 js-syplo d-block">{{ form_row(form.evalHousingGroup.syploId) }}</div>
        <div class="col-md-6 js-syplo d-block">{{ form_row(form.evalHousingGroup.syploDate) }}</div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">{{ form_row(form.evalHousingGroup.daloCommission) }}</div>
        <div class="col-md-6 js-daloCommission d-block">{{ form_row(form.evalHousingGroup.daloRecordDate) }}</div>
        <div class="col-md-6 js-daloCommission d-block">{{ form_row(form.evalHousingGroup.daloRequalifiedDaho) }}</div>
        <div class="col-md-6 js-daloCommission d-block">{{ form_row(form.evalHousingGroup.daloDecisionDate) }}</div>
        <div class="col-md-6 js-daloCommission d-block">{{ form_row(form.evalHousingGroup.daloTribunalAction) }}</div>
        <div class="col-md-6 js-daloTribunalAction d-block">{{ form_row(form.evalHousingGroup.daloTribunalActionDate) }}</div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">{{ form_row(form.evalHousingGroup.collectiveAgreementHousing) }}</div>
        <div class="col-md-6 js-collectiveAgreementHousing d-block">{{ form_row(form.evalHousingGroup.collectiveAgreementHousingDate) }}</div>
    </div>
    <div class="js-0_evalProfPerson_profStatus js-1_evalProfPerson_profStatus d-block">
        <div class="form-group row">
            <div class="col-md-6">{{ form_row(form.evalHousingGroup.hsgActionEligibility) }}</div>
            <div class="col-md-6 js-hsgActionEligibility d-block">{{ form_row(form.evalHousingGroup.hsgActionRecord) }}</div>
            <div class="col-md-6 js-hsgActionEligibility d-block">{{ form_row(form.evalHousingGroup.hsgActionDate) }}</div>
            <div class="col-md-6 js-hsgActionEligibility d-block">{{ form_row(form.evalHousingGroup.hsgActionDept) }}</div>
            <div class="col-md-6 js-hsgActionEligibility d-block">{{ form_row(form.evalHousingGroup.hsgActionRecordId) }}</div>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">{{ form_row(form.evalHousingGroup.housingExperience) }}</div>
        <div class="col-md-6 js-housingExperience d-block">{{ form_row(form.evalHousingGroup.housingExpeComment) }}</div>
    </div>

    <div class="form-group row js-housingStatus d-block">
        <div class="col-md-6">
            <div class="row">
                <div class="col-form-label col-sm-4">Type d'aides liées au logement</div>
                <div class="col-sm-8">
                    <select id="js-hsgHelps" name="hsgHelps" class="form-control">
                        <option class="default" value="">-- Ajouter --</option>
                        <option value="fsl">Fonds de solidarité pour le logement (FSL)</option>
                        <option value="fslEligibility">Eligibilité aide à l'installation FSL</option>
                        <option value="cafEligibility">Eligibilité CAF</option>
                        <option value="otherHelps">Autre(s) aide(s)</option>
                    </select>
                </div>
                <div class="col-md-12 mt-3">
                    <div class="table-responsive js-_evalSocialPerson_healthProblemType d-block">
                        <table class="table table-sm table-hover text-dark">
                            <tbody>
                                <tr id="js-hsgHelps-fsl" class="js-_evalHousingGroup_hsgHelps d-table-row">
                                    <th scope="row"><div class="sr-only">{{ form_widget(form.evalHousingGroup.fsl) }}</div></th>
                                    <td class="align-middle">Fonds de solidarité pour le logement (FSL)</td>
                                    <td><button class="btn btn-danger js-remove"><span class="fas fa-trash-alt"></span></button></td>
                                </tr>
                                <tr id="js-hsgHelps-fslEligibility" class="js-_evalHousingGroup_hsgHelps d-table-row">
                                    <th scope="row"><div class="sr-only">{{ form_widget(form.evalHousingGroup.fslEligibility) }}</div></th>
                                    <td class="align-middle">Eligibilité aide à l'installation FSL</td>
                                    <td><button class="btn btn-danger js-remove"><span class="fas fa-trash-alt"></span></button></td>
                                </tr>
                                <tr id="js-hsgHelps-cafEligibility" class="js-_evalHousingGroup_hsgHelps d-table-row">
                                    <th scope="row"><div class="sr-only">{{ form_widget(form.evalHousingGroup.cafEligibility) }}</div></th>
                                    <td class="align-middle">Eligibilité CAF</td>
                                    <td><button class="btn btn-danger js-remove"><span class="fas fa-trash-alt"></span></button></td>
                                </tr>
                                <tr id="js-hsgHelps-otherHelps" class="js-_evalHousingGroup_hsgHelps d-table-row">
                                    <th scope="row"><div class="sr-only">{{ form_widget(form.evalHousingGroup.otherHelps) }}</div></th>
                                    <td class="align-middle">{{ form_widget(form.evalHousingGroup.hepsPrecision) }}</td>
                                    <td><button class="btn btn-danger js-remove"><span class="fas fa-trash-alt"></span></button></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>		
            </div>
        </div>
    </div>

    <div class="form-group row">
        <div class="col-md-6">{{ form_row(form.evalHousingGroup.domiciliation) }}</div>
        <div class="col-md-6 js-domiciliation d-block">{{ form_row(form.evalHousingGroup.startDomiciliationDate) }}</div>
        <div class="col-md-6 js-domiciliation d-block">{{ form_row(form.evalHousingGroup.endDomiciliationDate) }}</div>
        <div class="col-md-6 js-domiciliation d-block">{{ form_row(form.evalHousingGroup.domiciliationAddress) }}</div>
        <div class="col-md-6 js-domiciliation d-block">{{ form_row(form.evalHousingGroup.domiciliationCity) }}</div>
        <div class="col-md-6 js-domiciliation d-block">{{ form_row(form.evalHousingGroup.domiciliationDept) }}</div>
    </div>
    <div class="form-group row">
        <div class="col-md-12">
            {{ form_widget(form.evalHousingGroup.commentEvalHousing) }}
        </div>
    </div>
<div>

{% endblock %}