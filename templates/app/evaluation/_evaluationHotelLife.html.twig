{% extends "model/_collapseCard.html.twig" %}

{% set id = "eval_hotel_life" %}

{% block card_title %}Vie à l'hôtel{% endblock %}

{% block card_body %}

	<div class="form-group row">
		<div class="col-md-12">{{ form_label(form.evalHotelLifeGroup.food) }}</div>
		<div class="col-md-12">{{ form_widget(form.evalHotelLifeGroup.food) }}</div>
	</div>
	<div class="form-group row">
		<div class="col-md-12">{{ form_label(form.evalHotelLifeGroup.clothing) }}</div>
		<div class="col-md-12">{{ form_widget(form.evalHotelLifeGroup.clothing) }}</div>
	</div>
	<div class="form-group row">
		<div class="col-md-12">{{ form_label(form.evalHotelLifeGroup.roomMaintenance) }}</div>
		<div class="col-md-12">{{ form_widget(form.evalHotelLifeGroup.roomMaintenance) }}</div>
	</div>
	<div class="form-group row">
		<div class="col-md-12">{{ form_label(form.evalHotelLifeGroup.otherHotelLife) }}</div>
		<div class="col-md-12">{{ form_widget(form.evalHotelLifeGroup.otherHotelLife) }}</div>
	</div>
	<div class="form-group row">
		<div class="col-md-12">{{ form_label(form.evalHotelLifeGroup.commentHotelLife) }}</div>
		<div class="col-md-12">{{ form_widget(form.evalHotelLifeGroup.commentHotelLife) }}</div>
	</div>

{% endblock %}
